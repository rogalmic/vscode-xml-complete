{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2018",

		"noImplicitAny": true,
		"removeComments": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"inlineSourceMap": false,
		"sourceMap": true,
		"outDir": "../out",
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedParameters": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
